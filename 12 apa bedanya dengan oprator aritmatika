<?php
$speed = 72;
$speed2 = 9;
// ini opertor aritmatika
$speed = $speed + 19;

// maka nilai speed akan samadengan 83 + 10 = 93

// ini operator penugasan
$speed += 9;

// sekarang nilai speed akan menjadi 83 + 10 = 93
print($speed)."<hr>;

$speed = $speed + 9;
// dan
$speed += 9;

print($speed2);
